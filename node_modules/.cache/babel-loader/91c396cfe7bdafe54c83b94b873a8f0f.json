lse>\n    <button @click=\"logout();\">로그아웃</button>\n  </div>\n\n  <router-link to=\"/home\">\n    Home\n  </router-link>\n  |\n  <router-link \n    v-if=\"typeof this.userId === 'undefined' || this.userId == null || this.userId === ''\" \n    to=\"/comm\">\n    게시판\n  </router-link>\n\n  <router-link v-else :to=\"{ path: '/comm/'+this.userId}\">\n    게시판\n  </router-link>\n\n  <div v-if=\"typeof this.userId != 'undefined' && this.userId != null && this.userId != ''\" >\n    <p class=\"login\">{{this.userId}}님 오늘도 방문해주셔서 감사합니다.</p>\n  </div>\n  <!-- <p>Selected page: {{page}}</p> -->\n  <router-view />\n  <!-- <div class=\"pagination\">\n    <pagenation \n      :records=\"10000\" \n      v-model=\"page\" \n      :per-page=\"100\" \n      @paginate=\"callback\">\n    </pagenation>   -->\n  <!-- </div> -->\n  \n</template>\n\n\n\n<script>\n// import HelloWorld from './components/HelloWorld.vue'\n\n// import Pagenation from 'vue-pagination-2';\n\nexport default {\n  name: 'App',\n  components: {\n    // HelloWorld,\n    // Pagenation\n  },\n  data() {\n    return {\n      page: 1,\n      userId: '' // 로그인 아이디\n    }\n  },\n  methods: {\n    // callback: function(page) {\n    //   console.log(`Page ${page} was selected. Do something about it`);\n    //   return page\n    // }\n    login(){\n      this.userId = this.tempUserId;\n    },\n    logout(){\n      this.userId = '';\n    },\n    refresh(){\n      this.$router.push('App');\n    }\n\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n"]},"metadata":{},"sourceType":"module"}